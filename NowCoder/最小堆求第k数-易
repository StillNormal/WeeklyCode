#include <iostream>
#include <queue> // stl只有最大堆 父节点大于左右叶子节点的值
#include <vector>
using namespace std;

int main() {
    int n, k ,tmp;
    while (cin >> n) { // 多组数据
        priority_queue<int> pq;
        for (int i = 0; i < n; i++) {
            cin >> tmp;
            pq.push(-1 * tmp); // 不是push_back 
        }
        cin >> k;
        for (int i = 0; i < k - 1; i++) {
            do {
                tmp = pq.top();
                pq.pop();
            } while (tmp == pq.top()); // 数组中有重复元素
        }
        cout << -1 * pq.top(); // Trick 最大堆转最小堆
    }
    return 0;
}